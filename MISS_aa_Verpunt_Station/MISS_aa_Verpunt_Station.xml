  <folder_arme name_arme="Comm Traffic" id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669" parent_id_arme="679ce8f8-c8cf-4a5c-97ca-92b46acc5a7d" expanded_arme="" />
  <event name_arme="Verpunt Station: Cleared for approach" id_arme="89c0c09b-b009-4c5e-83e7-5b0504751f33" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="0.0" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Verpunt Station Traffic Control">Enterprise, welcome to Verpunt Station. You are cleared for approach on your current vector. Please have your Comm officer notify us when you are ready to dock.</incoming_comms_text>
    <set_variable name="commTrafficChapter1" value="1.0" />
  </event>
  <event name_arme="Verpunt Station: Refueling" id_arme="38feb7c0-6a36-4b78-ace9-231c238effe2" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="1.0" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <if_docked name="Verpunt Station" />
    <incoming_comms_text from="Verpunt Station Ground Crew">Enterprise, we're refueling your reserves right now. Sorry we don't carry any armaments compatible with your systems, but we'll keep your fuel tanks topped off anytime you want to drop by.</incoming_comms_text>
  </event>
  <event name_arme="Docking Bay 01: Ambassador Troy is aboard" id_arme="d6b21793-3a22-4d48-a61f-4363083d6a08" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="1.0" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <if_docked name="Verpunt Station" />
    <incoming_comms_text from="Docking Bay 01">Lieutenant Chel to bridge. The ambassador is aboard and ready to get under way.</incoming_comms_text>
    <warning_popup_message message="SENSORS: Unusually high levels of verteron radiation present. Radiation is within safe limits." consoles="S" />
    <set_ship_text name="Verpunt Station" desc="Sensors show unusually high levels of ambient verteron radiation in proximity to the station." scan_desc="Unable to complete sensor sweep due to verteron radiation interference." />
    <set_variable name="commTrafficChapter1" value="2.0" />
    <set_variable name="isAmbassadorAboard" value="1.0" />
    <set_timer name="tmrNextCommTraffic" seconds="5" />
  </event>
  <event name_arme="Ambassador Troy: Ready to return to DS9" id_arme="844bc95e-819d-409e-8e21-aacc59e25bb1" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="2.0" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Ambassador Troy (Guest Quarters 11C)">Ambassador Troy to bridge. Captain, thank you for assigning Lieutenant Chel as our guide. I'm aware that some officers of the TSN are...uneasy around telepaths. It's nice to know that your crew are not among them.</incoming_comms_text>
    <set_timer name="tmrNextCommTraffic" seconds="15" />
    <set_variable name="commTrafficChapter1" value="3.0" />
  </event>
  <event name_arme="Verpunt Station: Contacts approaching A3" id_arme="be7e1db5-66a6-4836-83ea-9c1398082c77" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="3.0" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Verpunt Station Traffic Control">Enterprise, be advised: we are reading multiple unknown contacts bearing 200 near grid A3.</incoming_comms_text>
    <set_timer name="tmrNextCommTraffic" seconds="5" />
    <set_variable name="commTrafficChapter1" value="4.0" />
    <warning_popup_message message="Incoming message from Verpunt Station" consoles="M" />
  </event>
  <event name_arme="Ambassador Troy: Stay on mission" id_arme="e5cc4be0-c4cf-49e0-a94a-2c8787437f77" parent_id_arme="1c18f3c8-dd9f-41b9-bf1d-5d55076f3669">
    <if_variable name="commTrafficChapter1" comparator="EQUALS" value="4.0" />
    <if_timer_finished name="tmrNextCommTraffic" />
    <if_variable name="chapter" comparator="EQUALS" value="1.0" />
    <incoming_comms_text from="Ambassador Troy (Guest Quarters 11C)">Captain, your priority is still to deliver me and these treaty documents to DS9. Don't even think about chasing sensor ghosts right now. And-- oh, Captain, that was not a nice thought about telepaths. You were doing so well, too.^-- end transmission --</incoming_comms_text>
    <set_timer name="tmrNextCommTraffic" seconds="30" />
    <set_variable name="commTrafficChapter1" value="5.0" />
  </event>
